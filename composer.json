{
	"name" : "stinger-soft/platform-bundle",
	"type" : "symfony-bundle",
	"description" : "Basic bundle for all Stinger-Soft symfony based projects, bundling all kinds of platform functionality",
	"keywords" : [
		"Platform"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Oliver Kotte",
			"email" : "oliver.kotte@stinger-soft.net",
			"homepage" : "http://dev.koddistortion.net",
			"role" : "Developer"
		}, {
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\PlatformBundle\\" : ""
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0.x-dev",
			"dev-develop" : "1.1.x-dev"
		}
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : ">=5.5",
		"symfony/symfony" : "^3.0",
		"bmatzner/fontawesome-bundle" : "~4.6.1",
		"bmatzner/jquery-bundle" : "~2.2.2",
		"doctrine/orm" : "^2.5",
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/doctrine-cache-bundle" : "^1.2",
		"doctrine/doctrine-fixtures-bundle" : "~2.3.0",
		"doctrine/data-fixtures" : "~1.1.1",
		"incenteev/composer-parameter-handler" : "^2.0",
		"friendsofsymfony/user-bundle" : "~2.0@dev",
		"friendsofsymfony/jsrouting-bundle" : "~1.6",
		"knplabs/knp-menu-bundle" : "2.1.1",
		"knplabs/knp-paginator-bundle" : "~2.5",
		"knplabs/knp-components" : "~1.3",
		"knplabs/doctrine-behaviors" : "1.3.2",
		"leafo/scssphp" : "0.6.3",
		"sensio/distribution-bundle" : "^5.0",
		"sensio/framework-extra-bundle" : "^3.0.2",
